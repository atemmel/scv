// User type
struct User is trait1, trait2 {
	int age
	string username
	string desc
	string id
	Date date
}

// Date type
struct Date {
	u8 d
	u8 m
	u32 y
}

trait trait1 requires <iostream>, "format.hpp" {

	code {
void printt(const @Type& value) {
	@ForMemberIn(@Type) {
		std::cout << @Member << ' ';
	}
	std::cout << '\n';
}
	}

}
